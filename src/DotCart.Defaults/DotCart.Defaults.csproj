<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Contains the drivers for the default DotCart backing services</Title>
        <Description>DotCart toolbox offers out-of-the-box support for:
            - EventStoreDB
            - Redis
            - RabbitMQ
            - Core NATS (async request/reply)
        </Description>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\drivers\DotCart.Drivers.EventStoreDB\DotCart.Drivers.EventStoreDB.csproj"/>
        <ProjectReference Include="..\..\drivers\DotCart.Drivers.NATS\DotCart.Drivers.NATS.csproj"/>
        <ProjectReference Include="..\..\drivers\DotCart.Drivers.Redis\DotCart.Drivers.Redis.csproj"/>
        <ProjectReference Include="..\DotCart\DotCart.csproj" />
    </ItemGroup>


</Project>
